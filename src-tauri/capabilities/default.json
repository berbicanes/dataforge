{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "dialog:allow-save",
    "dialog:allow-open",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "updater:default",
    "process:allow-restart"
  ]
}
